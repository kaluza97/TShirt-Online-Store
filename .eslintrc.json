{
  "root": true,
  "env": {
    "browser": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module"
  },
  "extends": [
    "next",
    "next/core-web-vitals",
    "prettier"
  ],
  "rules": {
    "semi": [
      "error",
      "always"
    ],
    "no-unused-vars": "warn",
    "no-console": "warn",
    "no-undef": "error"
  }
}